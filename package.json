{
  "name": "fae",
  "version": "1.4.1",
  "description": "A tiny magical game framework written in ES6",
  "repository": "sambrosia/fae",
  "author": "Sam Woodruff",
  "license": "MIT",
  "main": "dist/fae.js",
  "module": "src/fae.js",
  "files": [
    "dist",
    "src"
  ],
  "esm": {
    "mode": "auto",
    "cjs": true
  },
  "ava": {
    "files": "**/*.test.js",
    "source": "src/**",
    "require": [
      "esm"
    ]
  },
  "scripts": {
    "test": "ava --watch",
    "build": "rollup --config",
    "build:watch": "rollup --config --watch",
    "docs": "docco src/*.js src/**/*.js",
    "prepublishOnly": "git push && git push --tags"
  },
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "not dead"
  ],
  "dependencies": {
    "eventemitter3": "^3.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.5",
    "@babel/preset-env": "^7.1.5",
    "ava": "^0.25.0",
    "docco": "^0.8.0",
    "esm": "^3.0.84",
    "rollup": "^0.67.0",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-filesize": "^5.0.1",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-uglify": "^6.0.0"
  }
}
