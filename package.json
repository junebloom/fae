{
  "name": "fae",
  "version": "1.4.0",
  "description": "A tiny magical game framework written in ES6",
  "repository": "sambrosia/fae",
  "author": "Sam Woodruff",
  "license": "MIT",
  "main": "dist/fae.js",
  "module": "src/fae.js",
  "files": ["dist", "src"],
  "esm": {
    "mode": "auto",
    "cjs": true
  },
  "ava": {
    "files": "**/*.test.js",
    "source": "src/**",
    "require": ["esm"]
  },
  "scripts": {
    "test": "ava --watch",
    "build": "rollup --config",
    "docs": "docco src/*.js src/**/*.js",
    "prepublishOnly": "git push && git push --tags"
  },
  "dependencies": {
    "eventemitter3": "^3.1.0"
  },
  "devDependencies": {
    "esm": "^3.0.28",
    "ava": "^0.25.0",
    "docco": "^0.8.0",
    "rollup": "^0.58.2",
    "rollup-plugin-buble": "^0.19.2",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-filesize": "^1.5.0",
    "rollup-plugin-json": "^2.3.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-uglify": "^3.0.0"
  }
}
